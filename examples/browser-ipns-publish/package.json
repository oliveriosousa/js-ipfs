{
  "name": "browser-ipns-publish",
  "version": "1.0.0",
  "description": "An example demonstrating publishing to IPNS in the browser",
  "private": true,
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./dist ./.cache ./.parcel-cache",
    "test": "test-ipfs-example",
    "build": "parcel build public/index.html --dist-dir ./dist",
    "serve": "parcel serve public/index.html",
    "start": "npm run serve"
  },
  "author": "Doug Anderson",
  "license": "MIT",
  "dependencies": {
    "human-crypto-keys": "^0.1.4",
    "ipfs": "^0.55.4",
    "ipfs-http-client": "^50.1.2",
    "ipfs-utils": "^8.1.2",
    "ipns": "^0.12.0",
    "it-last": "^1.0.4",
    "p-retry": "^4.2.0",
    "uint8arrays": "^2.1.3"
  },
  "browserslist": [
    "last 2 versions and not dead and > 2%"
  ],
  "devDependencies": {
    "delay": "^5.0.0",
    "execa": "^5.0.0",
    "ipfsd-ctl": "^8.0.1",
    "go-ipfs": "0.8.0",
    "parcel": "2.0.0-beta.2",
    "path": "^0.12.7",
    "test-ipfs-example": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ipfs/js-ipfs/examples"
  },
  "keywords": [
    "IPNS",
    "Publish"
